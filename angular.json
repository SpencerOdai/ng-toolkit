{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@ngx-toolkit/datatable": {
      "projectType": "library",
      "root": "projects/ngx-toolkit/datatable",
      "sourceRoot": "projects/ngx-toolkit/datatable/src",
      "prefix": "ngx-tk",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-toolkit/datatable/tsconfig.lib.json",
            "project": "projects/ngx-toolkit/datatable/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-toolkit/datatable/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-toolkit/datatable/src/test.ts",
            "tsConfig": "projects/ngx-toolkit/datatable/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-toolkit/datatable/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-toolkit/datatable/tsconfig.lib.json",
              "projects/ngx-toolkit/datatable/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-toolkit/shared": {
      "projectType": "library",
      "root": "projects/ngx-toolkit/shared",
      "sourceRoot": "projects/ngx-toolkit/shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-toolkit/shared/tsconfig.lib.json",
            "project": "projects/ngx-toolkit/shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-toolkit/shared/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-toolkit/shared/src/test.ts",
            "tsConfig": "projects/ngx-toolkit/shared/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-toolkit/shared/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-toolkit/shared/tsconfig.lib.json",
              "projects/ngx-toolkit/shared/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-toolkit/paginator": {
      "projectType": "library",
      "root": "projects/ngx-toolkit/paginator",
      "sourceRoot": "projects/ngx-toolkit/paginator/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-toolkit/paginator/tsconfig.lib.json",
            "project": "projects/ngx-toolkit/paginator/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-toolkit/paginator/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-toolkit/paginator/src/test.ts",
            "tsConfig": "projects/ngx-toolkit/paginator/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-toolkit/paginator/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-toolkit/paginator/tsconfig.lib.json",
              "projects/ngx-toolkit/paginator/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-toolkit/dialog": {
      "projectType": "library",
      "root": "projects/ngx-toolkit/dialog",
      "sourceRoot": "projects/ngx-toolkit/dialog/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-toolkit/dialog/tsconfig.lib.json",
            "project": "projects/ngx-toolkit/dialog/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-toolkit/dialog/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-toolkit/dialog/src/test.ts",
            "tsConfig": "projects/ngx-toolkit/dialog/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-toolkit/dialog/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-toolkit/dialog/tsconfig.lib.json",
              "projects/ngx-toolkit/dialog/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/demo",
      "sourceRoot": "projects/demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo",
            "index": "projects/demo/src/index.html",
            "main": "projects/demo/src/main.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets"
            ],
            "styles": [
              "projects/demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo/src/environments/environment.ts",
                  "with": "projects/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo/src/test.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.spec.json",
            "karmaConfig": "projects/demo/karma.conf.js",
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets"
            ],
            "styles": [
              "projects/demo/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo/tsconfig.app.json",
              "projects/demo/tsconfig.spec.json",
              "projects/demo/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/demo/e2e/protractor.conf.js",
            "devServerTarget": "demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    }
  },
  "defaultProject": "demo"
}
